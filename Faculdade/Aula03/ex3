positivo = 0
negativo = 0
par = 0
impar = 0

for i in range (1,6):
    numero = int(input("Digite o numero: "))
    if numero % 2 == 0:
        par+=1
    else:
        impar+=1
    if numero > 0:
        positivo+=1
    elif numero < 0: 
        negativo+=1

print(f"""Foram digitados: 
      {par} Números PAR
      {impar} Números IMPAR
      {positivo} Números POSITIVOS
      {negativo} Números NEGATIVOS
      """)