valor_1 = int(input("Digite o Primeiro Número: "))
valor_2 = int(input("Digite o Segundo Número: "))

soma = 0

if (valor_1 < valor_2):
    for i in range(valor_1+1, valor_2):
        if i % 2 != 0:
            soma+=i
elif (valor_2 < valor_1):
    for i in range(valor_2+1, valor_1):
        if i % 2 !=0:
            soma+=i
print(f"A Soma dos números Impares no intervalo é de: {soma}")