while True:
    print("-"*20)
    numero = int(input("Digite o NÃºmero: "))
    print("-"*20)
    if numero < 0:
        break
    else:
        for i in range(1,11):
            tabuada = i * numero
            print(f"{i} X {numero} = {tabuada}")

print("Programa Tabuada Encerrado!")
print("-"*20)